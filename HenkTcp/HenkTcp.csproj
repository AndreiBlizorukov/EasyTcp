<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Henkje</Authors>
    <Company>HenkSharp</Company>
    <Description>HenkTcp is a easy,async TcpServer/TcpClient library.</Description>
    <Version>0.0.0.2</Version>
    <PackageProjectUrl>https://github.com/GHenkje/HenkTcp/tree/master</PackageProjectUrl>
    <NeutralLanguage></NeutralLanguage>
    <PackageReleaseNotes></PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageTags>tcpclient, tcpserver, tcp, socket, server, client, HenkTcp</PackageTags>
    <AssemblyVersion>0.0.0.2</AssemblyVersion>
  </PropertyGroup>

</Project>
